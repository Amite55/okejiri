{
  "expo": {
    "name": "okejiriMobileApp",
    "slug": "okejiriMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashScreen.png",
    "scheme": "okejirimobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appAndroidIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.okejiriMobileApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appAndroidIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/androidSplashScreen.png",
          "imageWidth": 124,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
  [
    "expo-camera",
    {
      "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
      "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
      "recordAudioAndroid": true
    }
  ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
