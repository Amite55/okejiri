{
  "expo": {
    "name": "okejiriMobileApp",
    "slug": "okejiriMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashSquire.png",
    "scheme": "okejirimobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d07b47a6-f66a-4962-ab36-4b06e293ebd2"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yxltechnology.between",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appAndroidIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.okejiriMobileApp",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appAndroidIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashSquire.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-web-browser",
      "@react-native-community/datetimepicker",
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
