{
  "expo": {
    "name": "okejiriMobileApp",
    "slug": "okejiriMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashSquire.png",
    "scheme": "okejirimobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "googleAndroidClientId": "943955425216-30nr5pmpp7re3ve3kh5f8li0u3t0qul2.apps.googleusercontent.com",
      "googleWebClientId": "943955425216-s8s45vodqnobas78to08e1m8b3a8q7bm.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "d07b47a6-f66a-4962-ab36-4b06e293ebd2"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yxltechnology.between",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appAndroidIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.okejiriMobileApp",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appAndroidIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashSquire.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-web-browser",
      "@react-native-community/datetimepicker",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
